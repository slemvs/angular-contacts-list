{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "*": [
        "*",
        "app/models/*",
        "app/components/*",
        "app/*"
      ]
    },
    "target": "es5",
    "lib": [
      "es2016",
      "dom"
    ]
  }
}